+
+    GUID cur_type;
+    ON_SUCCEEDED(output_media->GetGUID(MF_MT_SUBTYPE, &cur_type));
+    if (FAILED(hr))
+      return hr;
+
+    if (cur_type == media_type) {
+      hr = decoder->SetOutputType(0, output_media.Get(), 0);
+      ON_SUCCEEDED(*type_found = true);
+      return hr;
+    }
+
+    type++;
